{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "A ParcelJS boilerplate with image compression and Vue Single Files components.",
  "main": "index.html",
  "bin": {
    "page": "./cli"
  },
  "scripts": {
    "page": "parcel $page --open",
    "build": "parcel build $page --public-url ./ --no-source-maps --experimental-scope-hoisting"
  },
  "keywords": [
    "parcel",
    "boilerplate",
    "images-optimization"
  ],
  "author": "Alex Mayol",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-imagemin": "^1.1.2",
    "postcss-modules": "^6.0.0"
  }
}
